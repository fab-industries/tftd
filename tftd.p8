--thought for the day (v0.04)
--by fab.industries

function _init()

 cls(0)
 t=0
 filt=0
 offset=1
 max_lin=0
 wsly=-400
 glitch_t=0
 glitch_newt=true
 
 cartdata("fabsh_tftd")
 tftd_cart=dget(1)
 if tftd_cart==0 then
  tftd=pick_thought()
  dset(1,thought_i)
 else
  tftd="+++ "..thoughts[tftd_cart].." +++"
 end
 
end

function _update()

 t+=1
 current_time()
 newday_check()
 move_wsl()

end

function _draw()

 cls(0)
 
 fi_logo()
 
 if filt>=120 then
 
  spr(0,31,12,8,3)
 
  print("\^w\^t"..clock,32,46,3)
  print(date,39,58,11)
  print_thought()
 
 end
 
 glitch()
 draw_wsl() 
 scanlines()
 vignette()
 
end
